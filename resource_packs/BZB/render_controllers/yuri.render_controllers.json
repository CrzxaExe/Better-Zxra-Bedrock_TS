{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.yuri": {
      "geometry": "Array.geometry[query.variant]",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": ["Array.skin[query.variant]"],
      "arrays": {
        "textures": {
          "Array.skin": ["Texture.normal", "Texture.combat", "Texture.cheerleader"]
        },
        "geometries": {
          "Array.geometry": ["Geometry.default", "Geometry.default", "Geometry.cheerleader"]
        }
      },
      "uv_anim": {
        "scale": [1, "1 / 25"],
        "offset": [0, "Math.mod(Math.floor(query.life_time * 8), 25) / 25"]
      }
    }
  }
}
