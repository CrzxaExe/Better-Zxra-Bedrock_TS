  "shop_panel" : {
    "type": "stack_panel",
    "size": ["100%", "100%"],
    "orientation": "vertical",
    "layer": 1,
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "scrolling_panel@common.scrolling_panel": {
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "$show_background": false,
          "size": [ "100%", "97%" ],
          "$scrolling_content": "server_form.shop_scrolling_content",
          "$scroll_size": [ 5, "100% - 4px" ],
          "$scrolling_pane_size": [ "100% - 4px", "100% - 2px" ],
          "$scrolling_pane_offset": [ 2, 10 ],
          "$scroll_bar_right_padding_size": [ 0, 0 ]
        }
      }
    ]
  },

  "shop_scrolling_content": {
    "type": "stack_panel",
    "size": [ "100% - 4px", "100%c" ],
    "orientation": "vertical",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "label_offset_panel": {
          "type": "panel",
          "size": ["100%", "100%c"],
          "controls": [
            {
              "main_label": {
                "type": "label",
                "offset": [2,2],
                "color": "$main_header_text_color",
                "size": ["100%", "default"],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "text": "#form_text"
              }
            }
          ]
        }
      },
      {
        "padding": {
          "type": "panel",
          "size": [ "100%", 4 ]
        }
      },
      {
        "wrapping_panel": {
          "type": "panel",
          "size": [ "100%", "100%c" ],
          "controls": [
            {
              "shop_dynamic_buttons_panel@server_form.shop_dynamic_buttons_panel": {}
            }
          ]
        }
      }
    ]
  },

  "user_panel" : {
    "type": "panel",
    "size": ["100%", "100%"],
    "offset": [ 0, "-5%" ],
    "layer": 1,
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "controls": [
      {
        "label_offset_panel": {
          "type": "panel",
          "offset": ["8.9%", "-2%"],
          "anchor_from": "center",
          "size": ["65%", "100%c"],
          "controls": [
            {
              "main_label": {
                "type": "label",
                "offset": [2,2],
                "size": ["100%", "default"],
                "anchor_from": "top_left",
                "anchor_to": "top_left",
                "text": "#form_text"
              }
            }
          ]
        }
      },
      {
        "scrolling_panel@common.scrolling_panel": {
          "anchor_to": "top_left",
          "anchor_from": "top_left",
          "$show_background": false,
          "size": [ "27%", "100%" ],
          "$scrolling_content": "server_form.user_scrolling_content",
          "$scroll_size": [ 2, "100% - 4px" ],
          "$scrolling_pane_size": [ "100% - 4px", "100% - 2px" ],
          "$scrolling_pane_offset": [ 2, 0 ],
          "$scroll_bar_right_padding_size": [ 0, 0 ]
        }
      },
      {
        "player_bg": {
          "type": "image",
          "texture": "textures/ui/position_label_cz",
          "size": [ 94, 70 ],
          "layer": 2,
          "anchor_from": "bottom_left",
          "anchor_to": "bottom_left",
          "offset": [ "-18% -5px", "-47%" ],
          "controls": [
            {
              "player_renderer_panel": {
                "type": "panel",
                "size": [ 64, 64 ],
                "offset": [ 0, -10 ],

                "controls": [
                  {
                    "player_renderer": {
                      "type": "custom",
                      "renderer": "live_player_renderer",
                      "property_bag": {
                        "#look_at_cursor": true
                      },
                      "animation_reset_name": "screen_animation_reset",
                      "anims": [
                        "@common.screen_exit_size_animation_push",
                        "@common.screen_exit_size_animation_pop",
                        "@common.screen_entrance_size_animation_push",
                        "@common.screen_entrance_size_animation_pop"
                      ],
                      "layer": 8,
                      "anchor_from": "center",
                      "anchor_to": "center"
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "user_scrolling_content": {
    "type": "panel",
