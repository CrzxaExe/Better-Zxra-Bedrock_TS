{
  "namespace": "cz_anim",
  
  // Waduh Ngapain bang buka ini?
  // Start Screen
  "start_screen_cz": {
    "anim_type": "wait",
    "duration": 1.7,
    "next": "@cz_anim.start_screen_cz1"
  },
  "start_screen_cz1": {
    "anim_type": "alpha",
    "easing": "out_quad",
    "duration": 1.3,
    "from": 1,
    "to": 0
  },

  "logo_cz": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0.5,
    "from": 0,
    "to": 1,
    "next": "@cz_anim.logo_cz1"
  },
  "logo_cz1": {
    "anim_type": "wait",
    "duration": 1.6,
    "next": "@cz_anim.logo_cz2"
  },
  "logo_cz2": {
    "anim_type": "alpha",
    "easing": "out_quad",
    "duration": 0.5,
    "from": 1,
    "to": 0.6,
    "next": "@cz_anim.logo_cz3"
  },
  "logo_cz3": {
    "anim_type": "alpha",
    "easing": "out_bounce",
    "duration": 0.1,
    "from": 0.6,
    "to": 1,
    "next": "@cz_anim.logo_cz4"
  },
  "logo_cz4": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0.7,
    "from": 1,
    "to": 0
  },

  "main_button_anims": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0,
    "from": 1,
    "to": 0,
    "next": "@cz_anim.main_button1"
  },
  "main_button1": {
    "anim_type": "wait",
    "duration": 3,
    "next": "@cz_anim.main_button2"
  },
  "main_button2": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0.8,
    "from": 0,
    "to": 1
  },
  
  "dev_panel_anim": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ -200, 0 ],
    "next": "@cz_anim.dev_1"
  },
  "dev_1": {
    "anim_type": "wait",
    "duration": 2.2,
    "next": "@cz_anim.dev_2"
  },
  "dev_2": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0.9,
    "from": [ -200, 0 ],
    "to": [ 19, -8 ]
  },
  
  "side_panel_anim": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 200, 0 ],
    "next": "@cz_anim.side_1"
  },
  "side_1": {
    "anim_type": "wait",
    "duration": 2.0,
    "next": "@cz_anim.side_2"
  },
  "side_2": {
    "anim_type": "offset",
    "easing": "in_quad",
    "duration": 1.2,
    "from": [ 200, 0 ],
    "to": [ 0, 0 ]
  },
  
  "toasting": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 0, 100 ],
    "next": "@cz_anim.toast1"
  },
  "toast1": {
    "anim_type": "wait",
    "easing": "in_out_quad",
    "duration": 2.3,
    "next": "@cz_anim.toast2"
  },
  "toast2": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 1.5,
    "from": [ 0, 100 ],
    "to": [ 0, -10 ],
    "next": "@cz_anim.toast3"
  },
  "toast3": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 1,
    "from": [ 0, -10 ],
    "to": [ 0, 0 ]
  },
  
  "new_menu": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ -200, 0 ],
    "next": "@cz_anim.mns1"
  },
  "mns1": {
    "anim_type": "wait",
    "easing": "in_out_quad",
    "duration": 2.3,
    "next": "@cz_anim.mns2"
  },
  "mns2": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 1.5,
    "from": [ -200, 0 ],
    "to": [ 10, 0 ],
    "next": "@cz_anim.mns3"
  },
  "mns3": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 1,
    "from": [ 10, 0 ],
    "to": [ 0, 0 ]
  },
  
  
  // Hud Screen
  "join": {
    "anim_type": "wait",
    "duration": 3.2,
    "next": "@cz_anim.join1"
  },
  "join1": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 1.3,
    "from": 1,
    "to": 0
  },

  "cz_chat_offset": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0.5,
    "from": [ -220, 0 ],
    "to": [ 0, 0 ],
    "next": "@cz_anim.chat1"
  },
  "chat1": {
    "anim_type": "wait",
    "duration": "$chat_item_lifetime",
    "next": "@cz_anim.chat2"
  },
  "chat2": {
    "anim_type": "offset",
    "easing": "in_out_quad",
    "duration": 0.7,
    "to": [ -420, 0 ],
    "from": [ 0, 0 ],
    "destroy_at_end": "chat_grid_item"
  },
  
  "cz_chat_alpha": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0.5,
    "from": 0,
    "to": 1,
    "next": "@cz_anim.cz_chat_alpha1"
  },
  "cz_chat_alpha1": {
    "anim_type": "wait",
    "duration": "$chat_item_lifetime",
    "next": "@cz_anim.cz_chat_alpha2"
  },
  "cz_chat_alpha2": {
    "anim_type": "alpha",
    "easing": "in_out_quad",
    "duration": 0.7,
    "from": 1,
    "to": 0,
    "destroy_at_end": "@hud.chat_grid_item"
  }
}